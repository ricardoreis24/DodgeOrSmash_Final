
<!DOCTYPE html>
<html>
<body>
    <div>
        @if (Model.Count > 0)
        {
        <ul>@{var count = 1;}
            @foreach (var s in Model)
            {

                <li>@count - @s.UserName - @s.Score Score Points</li>
                <hr />
                count++;
            }
        </ul>
        }
    </div>
</body>
</html>
